{
  "name": "@totalpave/deprecated-prop-type",
  "version": "1.0.1",
  "description": "React Prop Type to mark a prop as deprecated",
  "main": "dist/api.js",
  "types": "dist/api.d.ts",
  "publishConfig": {
    "registry": "https://registry.totalpave.com:48731/"
  },
  "scripts": {
    "lint": "eslint ./src ./spec",
    "lint:fix": "eslint --fix ./src ./spec",
    "docs": "typedoc --excludePrivate --includeVersion -readme ./README.md --out ./docs ./src/api.ts",
    "build": "npm run lint && tsc",
    "build:release": "tsc",
    "changelog": "auto-changelog --output CHANGELOG.md -p --release-summary --ignore-commit-pattern chore",
    "test": "npm run lint && jest",
    "preversion": "npm test",
    "version": "NODE_ENV=production npm run-script build:release && npm run-script docs && npm run changelog && git add CHANGELOG.md docs && git commit -m 'chore: changelog'",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/totalpave/deprecated-prop-type.git"
  },
  "keywords": [
    "prop-types",
    "react"
  ],
  "author": "Taras Mankovski <taras@frontside.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/totalpave/deprecated-prop-type/issues"
  },
  "homepage": "https://github.com/totalpave/deprecated-prop-type#readme",
  "devDependencies": {
    "@totalpave/eslint-plugin": "7.0.17",
    "prop-types": "15.8.1",
    "ts-node": "10.9.2",
    "typedoc": "0.28.15",
    "typescript": "5.9.3",
    "ts-jest": "29.4.6",
    "jest": "30.2.0",
    "@jest/types": "30.2.0",
    "@types/jest": "30.0.0",
    "typedoc-plugin-markdown": "4.9.0",
    "auto-changelog": "2.5.0"
  },
  "dependencies": {
    "@types/node": "24.10.2",
    "@types/prop-types": "15.7.15",
    "@types/warning": "3.0.3",
    "warning": "4.0.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
